<Window x:Class="Авторизация_и_регистрация.AdminPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Авторизация_и_регистрация"
        mc:Ignorable="d"
        Title="AdminPanel" Height="450" Width="800">
    <Grid Background="#FF87CEFA">
        <ListView x:Name="ЛистПользователей" Height="281" Margin="10,10,5,0" VerticalAlignment="Top" Width="Auto" SelectionChanged="ЛистПользователей_SelectionChanged" Foreground="#FF4169E1" Background="#FF4169E1">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button Content="Перейти к  авторизации" HorizontalAlignment="Left" Margin="10,334,0,0" VerticalAlignment="Top" Width="779" Height="44" Click="Button_Click" Background="{x:Null}" BorderBrush="{x:Null}" FontSize="16" FontFamily="Segoe UI Light"/>

    </Grid>
</Window>
